    "editor.lineHighlightBackground": "#242736",
    "editor.selectionBackground": "#50717CB4",
    "editor.selectionHighlightBackground": "#FFCC0020",
    "editorSuggestWidget.background": "#272539",
    "editorSuggestWidget.foreground": "#A6ACCD",
    "editorSuggestWidget.highlightForeground": "#80CBC4",
    "editorSuggestWidget.selectedBackground": "#343154",
    "editorSuggestWidget.border": "#FFFFFF10",
    "highlightedDelimetersForeground": "#FFCB6B"
